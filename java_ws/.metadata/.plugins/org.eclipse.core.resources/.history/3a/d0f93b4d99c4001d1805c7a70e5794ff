package com.day5;

public class forTest {

	public static void main(String[] args) {
		
		/*
		 for(초기식; 조건식; 증감식){
		 반복명령;
		 }
		 */
		for(int i = 0; i < 3; i++) {
			System.out.println("Hello java!");
		}
		System.out.println("-----------------------");
		for(int i = 0; i < 4; i++){
		System.out.println(i);
		} //for
		System.out.println("---끝---");

	}

}
