package com.day4;

public class Asdf {

	public static void main(String[] args) {
		int a=6, b=6;
		char ch='D';

		System.out.println("++b=>" +  ++b);  // 
		System.out.println("a++=>" +  a++ + ", b++=>"+ b++  + ", ++a=>"+ ++a);  //
		System.out.println("ch++=>" +  ch++ + ", ch++ =>"+ ch++ + ", ++ch="+ ++ch);  //

		System.out.println("a=" +a +", b="+ b+", ch="+ch); //

	}

}
